{
  "compilerOptions": {
    "module": "Node16",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strict": true,
    "noUnusedLocals": true,
    "moduleResolution": "node16",
    "sourceMap": false,
    "baseUrl": ".",
    "declaration": false,
    "skipLibCheck": true
  },
  "include": ["server.ts", "route.ts"]
}
